<%- include("partials/_header") %>


<h3><a href="municipios/register"  > Registra Datos</a> 
<div class="container mt-5">
  <div class="row">
    <div class="col-md-7">

      <% if (data.length > 0) { %>
      <table class="table table-dark table-bordered table-hover">
        
        <thead>
          <tr>
            <td>idMunicipio</td>
            <td>nombre_municipio</td>
            <td>nombre_depto</td>
            <td>fecha_fundacion</td>
            <td>colores_bandera</td>
            <td>nombre_himno_oficial</td>
            <td>idPersona_gobernante</td>
          </tr>
        </thead>
        <tbody>

           <% for(var i = 0; i < data.length; i++) {%>
          <tr>
            <td><%= (i + 1) %></td>
            <td><%= data[i].nombre_municipio %></td>
            <td><%= data[i].nombre_depto %></td>
            <td><%= data[i].fecha_fundacion %></td>
            <td><%= data[i].colores_bandera %></td>
            <td><%= data[i].nombre_himno_oficial %></td>
            <td><%= data[i].idPersona_gobernante %></td>
            <td class="d-flex gap-2">
              <a href="/municipios/edit/<%= data[i].idMunicipio %>" class="btn btn-info">
                Edit
              </a>
              <a href="/municipios/delete/<%= data[i].idMunicipio %>" class="btn btn-danger">
          data
              </a>
            </td>
          </tr>
          <% } %> 
        </tbody>
      </table>
      <% } %>
    </div>


<%- include("partials/_footer") %>
